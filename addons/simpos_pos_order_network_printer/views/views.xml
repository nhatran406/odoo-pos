<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <template id="assets" name="point_of_sale assets" inherit_id="point_of_sale.assets">
        <xpath expr="//script[@src='/pos_restaurant/static/src/js/multiprint.js']" position="after">
            <script type="text/javascript" src="/simpos_pos_order_network_printer/static/src/js/multiprint.js"></script>
        </xpath>
        <xpath expr="." position="inside">
            <script type="text/javascript" src="/simpos_pos_order_network_printer/static/src/js/printers.js"></script>
        </xpath>
        <xpath expr="//script[@src='/point_of_sale/static/src/js/screens.js']" position="after">
            <script type="text/javascript" src="/simpos_pos_order_network_printer/static/src/js/screens.js"></script>
        </xpath>
    </template>
</odoo>